<?xml version="1.0" encoding="UTF-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.2" version="1.2">
  <file original="" product-name="Neos.Workspace.Ui" source-language="en" datatype="plaintext" target-language="ar">
    <body>
      <trans-unit id="back" xml:space="preserve">
        <source>Back</source>
        <target state="translated">رجوع</target>
      </trans-unit>
      <trans-unit id="cancel" xml:space="preserve">
        <source>Cancel</source>
        <target state="translated">إلغاء</target>
      </trans-unit>
      <trans-unit id="pathCaption" xml:space="preserve">
        <source>Path</source>
        <target state="translated">مسار</target>
      </trans-unit>
      <trans-unit id="relativePathCaption" xml:space="preserve">
        <source>Relative Path</source>
        <target state="translated">مسار نسبي</target>
      </trans-unit>
      <!-- Management -->
      <trans-unit id="workspaces.label" xml:space="preserve">
        <source>Workspaces</source>
        <target state="translated">مساحات العمل</target>
      </trans-unit>
      <trans-unit id="workspaces.createNewWorkspace" xml:space="preserve">
        <source>Create new workspace</source>
        <target state="translated">إنشاء فضاء عمل جديد</target>
      </trans-unit>
      <trans-unit id="workspaces.createWorkspace" xml:space="preserve">
        <source>Create workspace</source>
        <target state="translated">إنشاء فضاء العمل</target>
      </trans-unit>
      <trans-unit id="workspaces.deleteWorkspace" xml:space="preserve">
        <source>Delete workspace "{0}"</source>
        <target state="needs-translation">حذف فضاء العمل</target>
      </trans-unit>
      <trans-unit id="workspaces.editWorkspace" xml:space="preserve">
        <source>Edit workspace</source>
        <target state="translated">تعديل مجال العمل</target>
      </trans-unit>
      <trans-unit id="workspaces.yesDeleteWorkspace" xml:space="preserve">
        <source>Yes, delete the workspace</source>
        <target state="translated">نعم، إحذف فضاء العمل</target>
      </trans-unit>
      <trans-unit id="workspaces.editWorkspace" xml:space="preserve">
        <source>Edit workspace "{0}"</source>
        <target state="translated">تعديل مجال العمل</target>
      </trans-unit>
      <trans-unit id="workspaces.personalWorkspace" xml:space="preserve">
        <source>Personal workspace</source>
        <target state="translated">فضاء العمل الشخصي</target>
      </trans-unit>
      <trans-unit id="workspaces.privateWorkspace" xml:space="preserve">
        <source>Private workspace</source>
        <target state="translated">مجال العمل الخاص</target>
      </trans-unit>
      <trans-unit id="workspaces.sharedWorkspace" xml:space="preserve">
        <source>Shared workspace</source>
        <target state="translated">فضاء العمل الداخلي</target>
      </trans-unit>
      <trans-unit id="workspaces.readonlyWorkspace" xml:space="preserve">
        <source>Read-only workspace</source>
        <target state="translated">فضاء العمل خاص بالقراءة فقط</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.title" xml:space="preserve">
        <source>Title</source>
        <target state="translated">العنوان</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.description" xml:space="preserve">
        <source>Description</source>
        <target state="translated">الوصف</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.baseWorkspace" xml:space="preserve">
        <source>Base workspace</source>
        <target state="translated">مجال العمل الأساسي</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.owner" xml:space="preserve">
        <source>Owner</source>
        <target state="translated">المالك</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.visibility" xml:space="preserve">
        <source>Visibility</source>
        <target state="translated">الظهور</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.visibility.private" xml:space="preserve">
        <source>Private</source>
        <target state="translated">خاص</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.visibility.private.help" xml:space="preserve">
        <source>Only reviewers and administrators can access and modify this workspace</source>
        <target state="translated">فضاء العمل هذا لا يمكن الوصول إليه وتعديله إلا من طرف المراجعين والمسؤولين فقط</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.visibility.shared" xml:space="preserve">
        <source>Shared</source>
        <target state="translated">داخلي</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.visibility.shared.help" xml:space="preserve">
        <source>Any logged in editor can see and modify this workspace.</source>
        <target state="translated">يمكن لأي محرر تسجيل الدخول رؤية وتعديل فضاء العمل هذا.</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.changes" xml:space="preserve">
        <source>Changes</source>
        <target state="translated">التغييرات</target>
      </trans-unit>
      <trans-unit id="workspaces.openPageInWorkspace" xml:space="preserve">
        <source>Open page in "{0}" workspace</source>
        <target state="translated">فتح الصفحة في فضاء العمل "{0}"</target>
      </trans-unit>
      <trans-unit id="workspaces.help.cantDeletePersonalWorkspace" xml:space="preserve">
        <source>This is your personal workspace which you can't delete.</source>
        <target state="translated">هذا هو فضاء العمل الخاص بك الذي لا يمكنك حذفه.</target>
      </trans-unit>
      <trans-unit id="workspaces.help.workspaceContainsChanges" xml:space="preserve">
        <source>The workspace contains changes. To delete, discard the changes first.</source>
        <target state="translated">يحتوي فضاء العمل على مجموعة من التغييرات. للحذف، يرجى تجاهل التغييرات أولاً.</target>
      </trans-unit>
      <trans-unit id="workspaces.help.cantRebaseWorkspaceContainsChanges" xml:space="preserve">
        <source>Workspace can't be rebased on a different workspace because it still contains changes.</source>
        <target state="translated">لا يمكن إعادة إنشاء هذا فضاء العمل على أساس فضاء عمل مختلف لأنه لا يزال يحتوي على التغييرات.</target>
      </trans-unit>
      <trans-unit id="workspaces.help.cantDeleteWorkspaceHasDependencies" xml:space="preserve">
        <source>The workspace cannot be deleted because other workspaces depend on it.</source>
        <target state="translated">لا يمكن حذف فضاء العمل لأن مجالات عمل أخرى تعتمد عليه.</target>
      </trans-unit>
      <trans-unit id="workspaces.help.cantDeleteNoPermission" xml:space="preserve">
        <source>You don't have the permissions for deleting this workspace.</source>
        <target state="translated">ليس لديك الصلاحية لحذف هذا الفضاء العمل.</target>
      </trans-unit>
      <trans-unit id="workspaces.dialog.confirmWorkspaceDeletion" xml:space="preserve">
        <source>Do you really want to delete the workspace "{0}"?</source>
        <target state="translated">هل تريد حقاً حذف فضاء العمل "{0}"؟</target>
      </trans-unit>
      <trans-unit id="workspaces.dialog.thisWillDeleteTheWorkspace" xml:space="preserve">
        <source>This will delete the workspace including all unpublished content. This operation cannot be undone.</source>
        <target state="translated">سيؤدي هذا إلى حذف فضاء العمل بما في ذلك كل شيء غير منشور. هذه العملية لا يمكن التراجع عنها.</target>
      </trans-unit>
      <trans-unit id="workspaces.description" xml:space="preserve">
        <source>This module contains the overview of all elements within the current workspace and it enables to continue the review and publishing workflow for them.</source>
        <target state="translated">هذه الوحدة النمطية يحتوي على نظرة عامة على جميع العناصر في فضاء العمل الحالية، وأنه يمكن مواصلة استعراض ونشر سير العمل لهم.</target>
      </trans-unit>
      <trans-unit id="workspaces.unpublishedChanges" xml:space="preserve">
        <source>Unpublished changes in workspace "{0}"</source>
        <target state="translated">التغييرات غير المنشورة في فضاء العمل "{0}"</target>
      </trans-unit>
      <trans-unit id="workspaces.changesCounts" xml:space="preserve">
        <source>additions: {new}, changes: {changed}, removals: {removed}</source>
        <target state="translated">الإضافات: {new}، التغييرات: {changed}، عمليات الإزالة: {removed}</target>
      </trans-unit>
      <trans-unit id="workspaces.review" xml:space="preserve">
        <source>Review</source>
        <target state="translated">المراجعة</target>
      </trans-unit>
      <trans-unit id="workspaces.discardSelectedChanges" xml:space="preserve">
        <source>Discard selected changes</source>
        <target state="translated">تجاهل التغييرات المحددة</target>
      </trans-unit>
      <trans-unit id="workspaces.discardAllChanges" xml:space="preserve">
        <source>Discard all changes</source>
        <target state="translated">تجاهل كل التغييرات</target>
      </trans-unit>
      <trans-unit id="workspaces.publishAllChanges" xml:space="preserve">
        <source>Publish all changes</source>
        <target state="translated">انشر كل التغييرات</target>
      </trans-unit>
      <trans-unit id="workspaces.publishAllChangesTo" xml:space="preserve">
        <source>Publish all changes to {0}</source>
        <target state="translated">نشر كافة التغييرات إلى {0}</target>
      </trans-unit>
      <trans-unit id="changedContentCaption" xml:space="preserve">
        <source>Changed Content</source>
        <target state="translated">المحتوى المتغير</target>
      </trans-unit>
      <trans-unit id="workspaces.legend.deleted" xml:space="preserve">
        <source>deleted</source>
        <target state="translated">حذف</target>
      </trans-unit>
      <trans-unit id="workspaces.legend.created" xml:space="preserve">
        <source>created</source>
        <target state="translated">أنشأت</target>
      </trans-unit>
      <trans-unit id="workspaces.legend.moved" xml:space="preserve">
        <source>moved</source>
        <target state="translated">نقل</target>
      </trans-unit>
      <trans-unit id="workspaces.legend.hidden" xml:space="preserve">
        <source>hidden</source>
        <target state="translated">مخفي</target>
      </trans-unit>
      <trans-unit id="workspaces.legend.edited" xml:space="preserve">
        <source>edited</source>
        <target state="translated">حررت</target>
      </trans-unit>
      <trans-unit id="workspaces.thereAreNoUnpublishedChanges" xml:space="preserve">
        <source>There are no unpublished changes in this workspace.</source>
        <target state="translated">ليس هناك أية تغييرات غير منشورة في فضاء العمل هذا.</target>
      </trans-unit>
      <trans-unit id="workspaces.discardAllChangesInWorkspaceConfirmation" xml:space="preserve">
        <source>Do you really want to discard all changes in the "{0}" workspace?</source>
        <target state="translated">هل تريد حقاً أن تتجاهل كافة التغييرات في فضاء العمل "{0}"؟</target>
      </trans-unit>
      <trans-unit id="workspaces.workspaceWithThisTitleAlreadyExists">
        <source>A workspace with this title already exists.</source>
        <target state="translated">فضاء العمل بنفس الاسم موجود مسبقاً.</target>
      </trans-unit>
      <trans-unit id="workspaces.workspaceHasBeenUpdated">
        <source>The workspace "{0}" has been updated.</source>
        <target state="translated">تم تحديث فضاء العمل "{0}".</target>
      </trans-unit>
      <trans-unit id="workspaces.workspaceCannotBeDeletedBecauseOfDependencies">
        <source>Workspace "{0}" cannot be deleted because the following workspaces are based on it: {1}</source>
        <target state="translated">لا يمكن حذف فضاء عمل "{0}" لأن فضاء العمل التالي يستند إليه: {1}</target>
      </trans-unit>
      <trans-unit id="workspaces.cantEditBecauseWorkspaceContainsChanges">
        <source>Your personal workspace contains changes, please publish or discard them first.</source>
        <target state="translated">فضاء العمل الشخصي الخاص بك يحتوي على تغييرات، يرجى نشرها أو تجاهلها أولاً.</target>
      </trans-unit>
      <group id="workspaces.workspaceCannotBeDeletedBecauseOfUnpublishedNodes" restype="x-gettext-plurals">
        <trans-unit id="workspaces.workspaceCannotBeDeletedBecauseOfUnpublishedNodes[0]">
          <source>Did not delete workspace "{0}" because it currently contains {1} nodes.</source>
          <target state="needs-translation">Did not delete workspace "{0}" because it currently contains {1} nodes.</target>
        </trans-unit>
        <trans-unit id="workspaces.workspaceCannotBeDeletedBecauseOfUnpublishedNodes[1]">
          <source>Did not delete workspace "{0}" because it currently contains {1} node.</source>
          <target state="needs-translation">Did not delete workspace "{0}" because it currently contains {1} node.</target>
        </trans-unit>
      </group>
      <trans-unit id="workspaces.workspaceHasBeenRemoved">
        <source>The workspace "{0}" has been removed.</source>
        <target state="translated">تم حذف فضاء العمل "{0}".</target>
      </trans-unit>
      <trans-unit id="workspaces.selectedChangeHasBeenPublished">
        <source>The selected change has been published.</source>
        <target state="translated">تم نشر التغيير المحدد.</target>
      </trans-unit>
      <trans-unit id="workspaces.selectedChangeHasBeenDiscarded">
        <source>The selected change has been discarded.</source>
        <target state="translated">تم تجاهل التغيير المحدد.</target>
      </trans-unit>
      <trans-unit id="workspaces.selectedChangesHaveBeenPublished">
        <source>The selected changes have been published.</source>
        <target state="translated">تم نشر التغييرات المحددة.</target>
      </trans-unit>
      <trans-unit id="workspaces.selectedChangesHaveBeenDiscarded">
        <source>The selected changes have been discarded.</source>
        <target state="translated">تم تجاهل التغييرات المحددة.</target>
      </trans-unit>
      <trans-unit id="workspaces.allChangesInWorkspaceHaveBeenPublished">
        <source>All changes from workspace "{0}" have been published to {1}.</source>
        <target state="translated">تم نشر جميع التغييرات من فضاء العمل "{0}" إلى {1}.</target>
      </trans-unit>
      <trans-unit id="workspaces.allChangesInWorkspaceHaveBeenDiscarded">
        <source>All changes from workspace "{0}" have been discarded.</source>
        <target state="translated">تم تجاهل كافة التغييرات في فضاء العمل "{0}".</target>
      </trans-unit>
      <trans-unit id="workspaces.reviewWorkspace.label" xml:space="preserve">
        <source>Review</source>
        <target state="needs-translation">المراجعة</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.lastModified" xml:space="preserve">
        <source>Last modified</source>
        <target state="needs-translation">آخر تعديل</target>
      </trans-unit>
      <trans-unit id="workspaces.workspace.status" xml:space="preserve">
        <source>Status</source>
        <target state="needs-translation">الحالة</target>
      </trans-unit>
      <trans-unit id="applyChanges" xml:space="preserve">
        <source>Apply changes</source>
        <target state="needs-translation">أعتماد التغيرات</target>
      </trans-unit>
      <trans-unit id="workspaces.dialog.rebase.node" xml:space="preserve">
        <source>Node</source>
        <target state="needs-translation">عقدة</target>
      </trans-unit>
    </body>
  </file>
